{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "crons": [
    {
      "path": "/api/process-batch",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/process-popular",
      "schedule": "0 7-23 * * *"
    },
    {
      "path": "/api/process-popular",
      "schedule": "0 0,2,4,6 * * *"
    }
  ]
}
